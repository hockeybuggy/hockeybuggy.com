{
  "name": "hockeybuggy.com",
  "description": "The personal website of Douglas Anderson",
  "version": "1.0.0",
  "author": "Douglas Anderson",
  "repository": "git@github.com:hockeybuggy/hockeybuggy.com.git",
  "license": "MIT",
  "private": true,
  "scripts": {
    "build": "next build",
    "prebuild": "ts-node ./scripts/build_blog_feed.ts && ts-node ./scripts/build_sitemap.ts",
    "start": "next start -p 4000",
    "dev": "next",
    "lint": "eslint \"**/*.ts*\"",
    "typecheck": "tsc -p . --skipLibCheck && rm tsconfig.tsbuildinfo",
    "format": "prettier --write \"**/*.ts*\"",
    "check-format": "prettier --check \"**/*.ts*\"",
    "test:prebuild": "jest --runInBand -c 'prebuild_tests/jest.config.js'",
    "test:e2e": "JEST_PUPPETEER_CONFIG=e2e_tests/jest-puppeteer.config.js jest --runInBand -c 'e2e_tests/jest.config.js'",
    "clean": "rm -r .next"
  },
  "dependencies": {
    "add": "^2.0.6",
    "classnames": "^2.5.1",
    "date-fns": "^3.6.0",
    "feed": "^4.2.2",
    "gray-matter": "^4.0.3",
    "markdown-it": "^14.1.0",
    "markdown-it-anchor": "^8.6.7",
    "markdown-it-prism": "^2.3.0",
    "next": "^14.2.10",
    "normalize.css": "^8.0.1",
    "plaiceholder": "^2.5.0",
    "query-string": "^9.1.0",
    "react": "^18.3.1",
    "react-dom": "^18.2.0",
    "sharp": "^0.33.5",
    "slugify": "^1.6.6",
    "ts-node": "^10.9.2"
  },
  "devDependencies": {
    "@next/eslint-plugin-next": "^14.2.5",
    "@types/classnames": "^2.3.1",
    "@types/expect-puppeteer": "^5.0.6",
    "@types/jest": "^29.5.12",
    "@types/jest-environment-puppeteer": "^5.0.6",
    "@types/lodash": "^4.14.202",
    "@types/markdown-it": "^14.1.2",
    "@types/node": "16.11.56",
    "@types/react": "18.3.10",
    "@types/react-dom": "18.2.19",
    "@types/react-helmet": "^6.1.11",
    "@typescript-eslint/eslint-plugin": "^5.62.0",
    "@typescript-eslint/parser": "^5.62.0",
    "dotenv": "^16.4.5",
    "eslint": "^8.57.1",
    "eslint-config-next": "14.2.4",
    "eslint-config-prettier": "^9.1.0",
    "eslint-config-react": "^1.1.7",
    "eslint-plugin-jsx-a11y": "^6.9.0",
    "eslint-plugin-react": "^7.33.2",
    "jest": "^29.7.0",
    "jest-puppeteer": "^9.0.2",
    "libxmljs": "^1.0.11",
    "prettier": "^3.2.5",
    "puppeteer": "^21.4.1",
    "sass": "^1.71.1",
    "ts-jest": "^29.2.5",
    "typescript": "^5.2.2"
  }
}
