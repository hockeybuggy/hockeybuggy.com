    "@fortawesome/react-fontawesome": "^0.2.0",
    "add": "^2.0.6",
    "classnames": "^2.3.2",
    "date-fns": "^2.30.0",
    "feed": "^4.2.2",
    "gray-matter": "^4.0.3",
    "markdown-it": "^13.0.1",
