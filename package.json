{
  "name": "hockeybuggy.com",
  "description": "The personal website of Douglas Anderson",
  "version": "1.0.0",
  "author": "Douglas Anderson",
  "repository": "git@github.com:hockeybuggy/hockeybuggy.com.git",
  "license": "MIT",
  "private": true,
  "scripts": {
    "build": "yarn prebuild && next build",
    "prebuild": "ts-node ./scripts/build_blog_feed.ts && ts-node ./scripts/build_sitemap.ts",
    "start": "next start -p 4000",
    "dev": "next",
    "lint": "eslint \"**/*.ts*\"",
    "typecheck": "tsc -p . --skipLibCheck && rm tsconfig.tsbuildinfo",
    "format": "prettier --write \"**/*.ts*\"",
    "check-format": "prettier --check \"**/*.ts*\"",
    "test:prebuild": "jest --runInBand -c 'prebuild_tests/jest.config.js'",
    "test:e2e": "JEST_PUPPETEER_CONFIG=e2e_tests/jest-puppeteer.config.js jest --runInBand -c 'e2e_tests/jest.config.js'",
    "clean": "rm -r .next"
  },
  "dependencies": {
    "add": "^2.0.6",
    "classnames": "^2.5.1",
    "date-fns": "^4.1.0",
    "feed": "^4.2.2",
    "globals": "^16.5.0",
    "gray-matter": "^4.0.3",
    "lodash": "^4.17.21",
    "markdown-it": "^14.1.0",
    "markdown-it-anchor": "^9.2.0",
    "markdown-it-prism": "^3.0.0",
    "next": "^15.5.4",
    "normalize.css": "^8.0.1",
    "plaiceholder": "^2.5.0",
    "query-string": "^9.2.1",
    "react": "^19.2.0",
    "react-dom": "^19.1.0",
    "sharp": "^0.34.2",
    "slugify": "^1.6.6",
    "ts-node": "^10.9.2"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.1",
    "@eslint/js": "^9.36.0",
    "@types/classnames": "^2.3.4",
    "@types/expect-puppeteer": "^5.0.6",
    "@types/jest": "^29.5.14",
    "@types/jest-environment-puppeteer": "^5.0.6",
    "@types/lodash": "^4.17.20",
    "@types/markdown-it": "^14.1.2",
    "@types/node": "16.11.56",
    "@types/react": "19.2.2",
    "@types/react-dom": "19.1.5",
    "@types/react-helmet": "^6.1.11",
    "dotenv": "^17.2.1",
    "eslint": "^9.39.1",
    "eslint-plugin-jest": "^29.0.1",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-react": "^7.37.5",
    "jest": "^29.7.0",
    "jest-puppeteer": "^11.0.0",
    "libxmljs": "^1.0.11",
    "prettier": "^3.6.2",
    "puppeteer": "^24.18.0",
    "sass": "^1.89.2",
    "ts-jest": "^29.3.4",
    "typescript": "^5.2.2",
    "typescript-eslint": "^8.46.2"
  },
  "packageManager": "yarn@4.5.0"
}
