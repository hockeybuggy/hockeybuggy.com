  "private": true,
  "scripts": {
    "build": "next build",
    "prebuild": "ts-node ./scripts/build_blog_feed.ts && ts-node ./scripts/build_sitemap.ts",
    "start": "next start",
    "dev": "next",
    "start": "next start -p 4000",
    "lint": "eslint \"**/*.ts*\"",
    "typecheck": "tsc -p . --skipLibCheck && rm tsconfig.tsbuildinfo",
    "format": "prettier --write \"**/*.ts*\"",
    "check-format": "prettier --check \"**/*.ts*\"",
    "test:e2e": "JEST_PUPPETEER_CONFIG=e2e_tests/jest-puppeteer.config.js jest --runInBand -c 'e2e_tests/jest.config.js'",
    "clean": "rm -r .next"
  },
